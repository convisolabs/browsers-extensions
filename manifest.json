{
  "name": "Conviso Platform Shortcuts",
  "description": "Easily navigate key platform features",
  "version": "0.0.1",
  "manifest_version": 3,

  "action": {
    "default_icon": "favicon.png",
    "default_popup": "index.html"
  },

  "permissions":[
    "tabs"
  ],

  "host_permissions":[
    "<all_urls>"
  ],

  "author": "Conviso"

}
